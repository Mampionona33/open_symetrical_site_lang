{
  "manifest_version": 3,
  "name": "open symetrical site lang",
  "description": "open the site over language in new tasbs",
  "version": "1.0",
  "permissions": [
    "storage",
    "clipboardWrite",
    "clipboardRead",
    "contextMenus",
    "activeTab",
    "notifications",
    "activeTab",
    "scripting",
    "webNavigation"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "defaul_icon": "img/icons8-open-view-in-new-tab-50.png",
    "default_title": "open the site over language in new tasbs"
  },
  "options_page": "./option_page/option_page.html",
  "icons": {
    "16": "img/icons8-open-view-in-new-tab-16.png",
    "48": "img/icons8-open-view-in-new-tab-48.png",
    "128": "img/icons8-open-view-in-new-tab-128.png"
  }
}
